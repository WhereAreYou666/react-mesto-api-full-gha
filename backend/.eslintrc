{
  "env": {
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [
    {
      "env": {
        "node": true
      },
      "files": [
        ".eslintrc.{js,cjs}"
    ],
    "parserOptions": {
      "sourceType": "script"
    }
  }
],
"parserOptions": {
  "ecmaVersion": "latest",
  "sourceType": "module"
},
"plugins": [
  "require"
],
"rules": {
  "no-underscore-dangle": [
    "error",
    {
      "allow": [
        "_id"
      ]
    }
  ]
}
}
